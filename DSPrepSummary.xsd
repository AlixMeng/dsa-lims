                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        laboratory.name, preparation_method.name AS Prepname, assignment.name AS Ordername, preparation_geometry.name AS Geoname, sample_type.name AS Stype, preparation.amount, 
                         preparation_unit.name AS PrepAmount, preparation.quantity, quantity_unit.name AS QuanUnit, sample.reference_date, sample.external_id, sample.number, preparation.fill_height_mm, preparation.comment, 
                         sample.wet_weight_g, sample.dry_weight_g, sample.volume_l, assignment.customer_contact_name, preparation.workflow_status_id, sample.lod_weight_start, sample.lod_weight_end, sample.lod_temperature, 
                         preparation_method.name_short
FROM            preparation LEFT OUTER JOIN
                         workflow_status ON preparation.workflow_status_id = workflow_status.id LEFT OUTER JOIN
                         assignment ON preparation.assignment_id = assignment.id LEFT OUTER JOIN
                         preparation_unit ON preparation.prep_unit_id = preparation_unit.id RIGHT OUTER JOIN
                         sample ON preparation.sample_id = sample.id LEFT OUTER JOIN
                         quantity_unit ON preparation.quantity_unit_id = quantity_unit.id LEFT OUTER JOIN
                         sample_type ON sample.sample_type_id = sample_type.id LEFT OUTER JOIN
                         preparation_method ON preparation.preparation_method_id = preparation_method.id LEFT OUTER JOIN
                         preparation_geometry ON preparation.preparation_geometry_id = preparation_geometry.id LEFT OUTER JOIN
                         laboratory ON preparation.laboratory_id = laboratory.id
WHERE        (assignment.id = @aid) AND (preparation.instance_status_id = 1)
ORDER BY sample.number</CommandText>
